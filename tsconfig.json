{
  "compilerOptions": {
    "module": "commonjs",
    "allowJs": true,
    "noImplicitAny": true,
    "outDir": "./dist",
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "target": "es5"
  },
  "include": [
    "./src/**/*",
    "./vuepress/**/*"
  ],
  "exclude": [
    "node_modules",
    "vuepress"
  ]
}
